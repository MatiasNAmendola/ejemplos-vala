int main()
{
	int8 x = 10;
	int16 y = 10;
	uint8 z = 10;

	int j = 100;
	short i = 100;

	stdout.printf("int8= %d\n", x.MAX);
	stdout.printf("int16= %d\n", y.MAX);
	stdout.printf("uint8= %u\n", z.MAX);
	stdout.printf("short integer= %d\n", i.MAX);
	stdout.printf("integer= %d\n", j.MAX);
	return 0;
}
